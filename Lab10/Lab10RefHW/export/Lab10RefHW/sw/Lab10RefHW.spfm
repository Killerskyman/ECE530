<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="Lab10RefHW"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
Lab10RefHW
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="Lab10RefHW">
        <sdx:configuration sdx:name="Lab10RefHW"
                           sdx:displayName="Lab10RefHW"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>Lab10RefHW</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="Lab10RefHW/boot/Lab10RefHW.bif"
                            sdx:readme="Lab10RefHW/boot/generic.readme"
                            sdx:qemuArguments="Lab10RefHW/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="Lab10RefHW/boot/Lab10RefHW.bif"
                            sdx:readme="Lab10RefHW/boot/generic.readme"
                            sdx:qemuArguments="Lab10RefHW/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="Lab10RefHW/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="Lab10RefHW/standalone_domain/bspinclude/include"
                        sdx:bspConfig="Lab10RefHW/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
